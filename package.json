    "watch-css": "nodemon -e scss -x \"npm run build-css\""
  },
  "dependencies": {
    "ethereumjs-tx": "^1.3.6",
    "ethereumjs-util": "^5.2.0",
    "express": "^4.16.3",
    "linebot": "^1.4.1",
    "nedb": "^1.8.0",
    "nem-sdk": "^1.6.6",
    "web3": "^0.20.6"
  }
}
